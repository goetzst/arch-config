URxvt.font: xft:Terminus-16,xft:DejaVu Sans Mono:pixelsize=16,xft:unifont,xft:STIX,xft:STIX Math

URxvt.saveLines:        12000
URxvt.scrollBar:        false
URxvt.urgentOnBell:     true

!fontsize stuff
URxvt.perl-ext-common: font-size
URxvt.keysym.C-Up:     font-size:increase
URxvt.keysym.C-Down:   font-size:decrease
URxvt.keysym.C-equal:  font-size:reset

! for using the clipboard
URxvt.iso14755: false

! rofi
rofi.lines: 20
rofi.width: 40
rofi.columns: 3
rofi.font: terminus 16
rofi.bw: 2
rofi.color-window: #0e1112,#75b2b3,#75b2b3
rofi.color-normal: #0e1112,#ddccbb,#0B0E0E,#323232,#ddccbb
rofi.hide-scrollbar: true
rofi.terminal: urxvtc

! ???
Xcursor.size: 16

